  {
    "name": "Product grid",
    "settings": [
      {
        "type": "header",
        "content": "General"
      },
      {
        "type": "checkbox",
        "id": "show-brand-names",
        "label": "Show brand names / links",
        "default": true
      },
      {
        "type": "header",
        "content": "Products"
      },
      {
        "type": "checkbox",
        "id": "image-borders",
        "label": "Show product image borders",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "alt-product-card",
        "label": "Enable product details overlay",
        "default": false
      },
      {
        "type": "select",
        "id": "product_grid_aspect_ratio",
        "label": "Product grid aspect ratio",
        "options": [
          {
            "value": "default",
            "label": "Default"
          },
          {
            "value": "square",
            "label": "Square"
          },
          {
            "value": "tall",
            "label": "Tall"
          },
          {
            "value": "taller",
            "label": "Taller"
          },
          {
            "value": "wide",
            "label": "Wide"
          }
        ],
        "default": "default"
      },
      {
        "type": "header",
        "content": "Quick shop"
      },
      {
        "type": "checkbox",
        "id": "enable-quick-shop",
        "label": "Enable quick shop",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "quickshop-product-quantity-option",
        "label": "Show quantity selector",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "quickshop-product-email-link",
        "label": "Show email link",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "quickshop-product-quick-add",
        "label": "Enable quick add-to-cart",
        "default": false
      },
      {
        "type": "paragraph",
        "content": "Stay on the current page when an item is added to the cart"
      },
      {
        "type": "select",
        "id": "quickshop-thumb-orientation",
        "label": "Product thumbnail position",
