{
  "name": "frissky",
  "version": "1.0.0",
  "description": "Online beauty app",
  "scripts": {
    "build:js": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build": "npm run build:js",
    "dev:js": "webpack --watch --config webpack.config.dev.js",
    "dev:js:single": "webpack --config webpack.config.dev.js",
    "dev:css": "open -a 'Prepros'",
    "dev:theme": "theme watch",
    "dev": "npm run dev:css && concurrently -p \"[{name}]\" -n \"SHOPIFY,JS\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:theme\" \"npm run dev:js\"",
    "deploy": "theme upload",
    "start": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
